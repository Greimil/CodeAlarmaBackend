// Commandos para actualizar la BBDD 
// npx prisma migrate dev -- name (crea la migracion)
// npx prisma generate (sincroniza la base de datos con el modelo)


generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
}



model processedEvents {
  id Int @id @default(autoincrement())
  eventID String 
  createdAt DateTime
  processedAt DateTime
  operator String
  operatorNotes String?
  accountId String 
  code String 
  accountObservation String?
  evaluacionLlamada String
  cumplimientoProtocolo String
  esFaltaRecurrente Boolean
  cumpleSLA Boolean
  puntuacionLlamada Int
  evaluacionQA Int
  accionRecomendada String
  // callTranscription 
}